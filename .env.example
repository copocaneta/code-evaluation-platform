# Azure OpenAI Configuration
OPENAI_API_KEY=your_azure_openai_api_key_here
NEXT_PUBLIC_APP_NAME=Code Evaluation Platform

# Azure OpenAI Endpoint Configuration (optional - defaults in openai.ts)
AZURE_OPENAI_ENDPOINT=https://your-endpoint.openai.azure.com
AZURE_OPENAI_DEPLOYMENT=deployment-name
AZURE_OPENAI_API_VERSION=2024-02-15-preview

# Rate Limiter Configuration (optional - defaults in rateLimiter.ts)
RATE_LIMIT_CAPACITY=10
RATE_LIMIT_REFILL_RATE=0.1
RATE_LIMIT_REFILL_TIME=10

# Development Configuration
NODE_ENV=development 